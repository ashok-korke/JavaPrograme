pipeline{
agent any
stages{
     stage('Build'){
          steps{
                echo "Project building successfully...."
              }
    }
    stage('deploy'){
      steps{
              echo "Project Deployed successfully....."
           }
    }
    stage('Test'){
     steps{
           echo "Project Tested successfully......"
         }
    }
    stage('Release'){
      steps{
             echo "Project released successfully....."
           }
    }
    
}
}
